{
  "name": "protobox",
  "version": "1.0.0",
  "description": "Prototyping environment for development with React",
  "type": "module",
  "bin": {
    "pbox": "./dist/cli.js"
  },
  "main": "./dist/libs/index.js",
  "types": "./dist/libs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/libs/index.d.ts",
      "default": "./dist/libs/index.js"
    },
    "./*": {
      "types": "./dist/libs/*.d.ts",
      "default": "./dist/libs/*.js"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "npm run build:cli && npm run build:libs && npm run build:types && npm run build:preview",
    "build:types": "tsc -p tsconfig.libs.json",
    "build:cli": "vite build --config vite.cli.config.ts",
    "build:libs": "vite build --config vite.libs.config.ts",
    "build:preview": "vite build --config vite.preview.config.ts",
    "dev:preview": "vite --config vite.preview.config.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "dependencies": {
    "immer": "^10.0.3",
    "sass": "^1.69.5"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  }
}
