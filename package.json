{
  "name": "proto",
  "version": "1.0.0",
  "description": "Prototyping framework for rapid development with React",
  "type": "module",
  "bin": {
    "proto": "./dist/cli.js"
  },
  "main": "./dist/libs/index.js",
  "exports": {
    ".": "./dist/libs/index.js",
    "./context": "./dist/libs/context.js",
    "./parameters": "./dist/libs/parameters.js",
    "./useProtoParams": "./dist/libs/useProtoParams.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "npm run build:cli && npm run build:libs && npm run build:preview",
    "build:cli": "vite build --config vite.cli.config.ts",
    "build:libs": "vite build --config vite.libs.config.ts",
    "build:preview": "vite build --config vite.preview.config.ts",
    "dev:preview": "vite --config vite.preview.config.ts"
  },
  "dependencies": {
    "immer": "^10.0.3",
    "sass": "^1.69.5"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  }
}
