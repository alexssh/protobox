{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
		"strictNullChecks": true,
    "strictPropertyInitialization": true,
		"noImplicitAny": false,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "protobox": ["../protobox/dist/libs/index.d.ts"],
      "protobox/*": ["../protobox/dist/libs/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
